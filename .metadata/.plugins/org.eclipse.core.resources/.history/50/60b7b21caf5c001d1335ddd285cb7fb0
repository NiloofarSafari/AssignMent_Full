package october30;

public class One_dimensional_array {

	public static void main(String[] args) {
	 
		//Declaration 
		int[] a; // in behtarin soorate
		int a1[];
		int []a2;
		int[]a3;
		
		int[] a4,b4; //both are 1-D array
		int []a5,b5; //both are 1-D array
		int[]a7,b7; // both
		int a6[], b6; // only a6 is 1-D array. b6 is an int variable.
		
		//creation
		
		int[] a8 = new int[3];
		System.out.println(a8.length);
		
		int[] a9 = new int[0];
		System.out.println(a9.length);
		
		//int[] a10 = new int[-1];
		//System.out.println(a10.length); // negative exception array error
		
		// Initialization
		
		int[] a11 = new int[4];
		a11[0] = 123;
		a11[1] = 124;
		a11[2] = 125;
		a11[3] = 126; // ta inja doroste age eshtebahi bezani [4] unvaght error mide Index 4 Out of Bounds for Length 4
		//a11[4] = 127; is wrong      arrayIndexoutofBoundException   error
		System.out.println(a11.length);
		
		int[] a12 = {12,13,14,15,16};
		System.out.println(a12.length);
		
		int[] a13 = new int[] {10, 20, 30, 40, 50, 60};
		System.out.println(a13.length);
		
		//Retrieve 
		int[] a14= {999,888,777,666,555,444,333,222,111};
		// how can i print all the values in my output console?
		for(int i=0; i<a14.length; i++); {

			System.out.print(a14[i]);
			
		}
		
	}
	}


