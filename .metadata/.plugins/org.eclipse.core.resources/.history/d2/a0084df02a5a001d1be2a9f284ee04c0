			System.out.print(i + " ");
		
		//first 10 multiples of 5 
		//for (int k=5 ; k<=50 ; k++) { dorost nist
		//System.out.print(k);
		
	     	//number from 100 to 50
			// multiples of 5 in a decreasing order starting from 200 till 100
			//sum of first 100 natural number
